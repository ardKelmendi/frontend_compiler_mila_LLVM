program indirectrecursion;
var even: integer;
    odd : integer;
forward isodd(n: integer): integer;
function iseven(n: integer): integer;
begin
    if n > 0 then
    begin
        even = isodd(n - 1);
    end
    even = 1;
end

function isodd(n: integer): integer;
begin
    if n > 0 then
    begin
        odd = iseven(n - 1);
    end
    odd := 0;
end

begin
    writeln(iseven(11));
    writeln(isodd(11));
end.
